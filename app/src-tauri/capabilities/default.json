{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "shell:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": true,
          "name": "binaries/server",
          "sidecar": true
        },
        {
          "args": [
            "-m",
            {
              "validator": ".*"
            },
            "--port",
            {
              "validator": "\\d+"
            },
            "--api-key",
            {
              "validator": ".*"
            },
            "--reasoning-format",
            "none",
            "-np",
            "3",
            "--ctx-size",
            "4096",
            "-ctk",
            "q8_0",
            "-ctv",
            "q8_0",
            "--mlock",
            "-fa",
            "--no-webui",
            "--log-disable",
            "--jinja"
          ],
          "name": "binaries/server",
          "sidecar": true
        },
        {
          "args": true,
          "name": "llama",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-base.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-alderlake.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-haswell.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-icelake.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-sandybridge.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-sapphirerapids.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-skylakex.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-sse42.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-x64.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-rpc.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/libcurl-x64.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/llama.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/libomp140.x86_64.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/mtmd.dll",
          "sidecar": true
        }
      ]
    },
    "fs:default",
    "core:event:default",
    "deep-link:default",
    "log:default"
  ]
}