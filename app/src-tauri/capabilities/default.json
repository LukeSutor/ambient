{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main",
    "secondary",
    "screen-selector",
    "computer-use"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-create",
    "core:window:allow-close",
    "core:window:allow-set-focus",
    "core:window:allow-show",
    "core:window:allow-hide",
    "core:window:allow-set-size",
    "core:webview:allow-create-webview-window",
    "core:webview:allow-webview-close",
    "core:webview:default",
    "core:app:allow-app-hide",
    "core:app:allow-app-show",
    "core:window:allow-destroy",
    "core:window:allow-start-dragging",
    "opener:default",
    "shell:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": true,
          "name": "binaries/embedding",
          "sidecar": true
        },
        {
          "args": [
            "-m",
            {
              "validator": ".*"
            },
            "-p",
            {
              "validator": ".*"
            },
            "--ctx-size",
            "2048"
          ],
          "name": "binaries/server",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/server",
          "sidecar": true
        },
        {
          "args": [
            "-m",
            {
              "validator": ".*"
            },
            "-mm",
            {
              "validator": ".*"
            },
            "--port",
            {
              "validator": "\\d+"
            },
            "--api-key",
            {
              "validator": ".*"
            },
            "--reasoning-format",
            "none",
            "-np",
            "3",
            "--ctx-size",
            "4096",
            "-ctk",
            "q8_0",
            "-ctv",
            "q8_0",
            "--mlock",
            "-fa",
            "on",
            "--no-webui",
            "--log-disable",
            "--offline",
            "--jinja"
          ],
          "name": "binaries/server",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-base.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-alderlake.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-haswell.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-icelake.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-sandybridge.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-sapphirerapids.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-skylakex.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-sse42.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-x64.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-zen4.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-piledriver.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-ivybridge.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-cooperlake.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-cascadelake.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-cpu-cannonlake.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml-rpc.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/ggml.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/libcurl-x64.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/llama.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/libomp140.x86_64.dll",
          "sidecar": true
        },
        {
          "args": true,
          "name": "binaries/mtmd.dll",
          "sidecar": true
        }
      ]
    },
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        {
          "url": "https://google.com"
        }
      ]
    },
    "fs:default",
    "core:event:default",
    "deep-link:default",
    "log:default",
    "store:default",
    "os:default"
  ]
}