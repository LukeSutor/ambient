{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "analysis": {
      "type": "string",
      "description": "Explanation of why steps were completed (max 2 sentences)"
    },
    "completed": {
      "type": "array",
      "items": {
        "type": "integer",
        "description": "Step IDs that were completed"
      },
      "description": "Array of completed step IDs"
    }
  },
  "required": ["analysis", "completed"],
  "additionalProperties": false
}
