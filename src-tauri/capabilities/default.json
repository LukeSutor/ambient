{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": false,
          "name": "binaries/qwen2vl",
          "sidecar": true
        }
      ]
    },
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://**/"
        },
        {
          "url": "https://**/"
        }
      ]
    },
    "shell:default",
    "shell:allow-open",
    "http:default",
    "fs:default",
    "store:default"
  ]
}